<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Application Portal</title>
    <style>
        /* Same as before */

        .navbar {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 10px;
            background-color: #333;
            color: #fff;
        }

        .nav-item {
            cursor: pointer;
        }

        .response-list {
            list-style: none;
            padding: 0;
        }

        .response-item {
            margin-bottom: 10px;
        }
    </style>
</head>
<body>
    <div class="navbar">
        <div class="nav-item" onclick="showAccessCodeInput()">Access Code</div>
        <div class="nav-item" onclick="showMainPage()">Main Page</div>
    </div>

    <div class="container">
        <div class="applications" id="applications">
            <!-- Applications will be dynamically added here -->
            <div class="application" onclick="openForm('form1')">Application 1</div>
            <div class="application" onclick="openForm('form2')">Application 2</div>
            <!-- Add more applications as needed -->
        </div>

        <div class="editor" id="editor">
            <h2>Editor</h2>
            <!-- Placeholder for form responses -->
            <ul class="response-list" id="formResponses">
                <!-- Form responses will be dynamically added here -->
            </ul>
        </div>

        <div class="access-code-container" id="accessCodeContainer">
            <h2>Access Code</h2>
            <input type="text" class="access-code-input" id="accessCode" placeholder="Enter Access Code">
            <button class="access-code-button" onclick="verifyAccessCode()">Access</button>
        </div>

        <div id="formContainer">
            <!-- Form content will be dynamically added here -->
        </div>
    </div>

    <script>
        function verifyAccessCode() {
            var accessCode = document.getElementById('accessCode').value;

            // Placeholder: Replace this with actual access code verification logic on the server side
            if (accessCode === 'your_access_code') {
                showMainPage();
            } else {
                alert('Invalid access code');
            }
        }

        function showAccessCodeInput() {
            document.getElementById('accessCodeContainer').style.display = 'block';
            document.getElementById('mainPage').style.display = 'none';
            document.getElementById('editor').style.display = 'none';
        }

        function showMainPage() {
            document.getElementById('accessCodeContainer').style.display = 'none';
            document.getElementById('mainPage').style.display = 'block';
            document.getElementById('editor').style.display = 'none';
        }

        function showEditor() {
            document.getElementById('accessCodeContainer').style.display = 'none';
            document.getElementById('mainPage').style.display = 'none';
            document.getElementById('editor').style.display = 'block';

            // Placeholder: Fetch and display form responses
            fetchFormResponses();
        }

        function fetchFormResponses() {
            // Simulating fetching form responses from the server
            var formResponsesContainer = document.getElementById('formResponses');
            formResponsesContainer.innerHTML = ''; // Clear previous responses

            var responses = [
                "Response 1",
                "Response 2",
                "Response 3"
                // Add more responses as needed
            ];

            responses.forEach(function (response, index) {
                var listItem = document.createElement('li');
                listItem.className = 'response-item';
                listItem.textContent = response;
                formResponsesContainer.appendChild(listItem);
            });
        }

        <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Application Form</title>
    <style>
        body {
            font-family: 'Arial', sans-serif;
            background-color: #f4f4f4;
            margin: 0;
            padding: 20px;
        }

        .container {
            max-width: 600px;
            margin: auto;
            background-color: #fff;
            padding: 20px;
            border-radius: 8px;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        }

        label {
            display: block;
            margin-bottom: 8px;
        }

        input, textarea {
            width: 100%;
            padding: 10px;
            margin-bottom: 16px;
            box-sizing: border-box;
            border: 1px solid #ccc;
            border-radius: 4px;
        }

        button {
            background-color: #3498db;
            color: #fff;
            padding: 10px 20px;
            border: none;
            border-radius: 4px;
            cursor: pointer;
        }

        button:hover {
            background-color: #2980b9;
        }
    </style>
</head>
<body>
    <div class="container">
        <h2>Application Form</h2>
        <form id="applicationForm">
            <label for="fullName">Full Name:</label>
            <input type="text" id="fullName" name="fullName" required>

            <label for="email">Email:</label>
            <input type="email" id="email" name="email" required>

            <label for="phone">Phone:</label>
            <input type="tel" id="phone" name="phone" required>

            <label for="message">Message:</label>
            <textarea id="message" name="message" rows="4" required></textarea>

            <button type="submit">Submit Application</button>
        </form>
    </div>
</body>
</html>

        }
    </script>
</body>
</html>
